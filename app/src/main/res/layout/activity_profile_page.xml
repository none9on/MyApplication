    <TextView
        android:id="@+id/textView5"
        android:layout_width="304dp"
        android:layout_height="51dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="88dp"
        android:fontFamily="@font/anek_latin_semibold"
        android:text="Мой профиль"
        android:textColor="@color/black"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewProfileInformation"
        android:layout_width="201dp"
        android:layout_height="39dp"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="456dp"
        android:fontFamily="@font/anek_latin_semibold"
        android:text="Обзор профиля"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/search_page_bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.003"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="191dp"
        android:layout_height="36dp"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="420dp"
        android:fontFamily="@font/anek_latin_semibold"
        android:text="История заказов"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/search_page_bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewProfileInformation"
        app:layout_constraintVertical_bias="0.533" />

</androidx.constraintlayout.widget.ConstraintLayout>